#%RAML 1.0 DataType
type: object
description: Common Datatype for all SAP Outbound interfaces which takes a .zip file and EDI Message Header Inforamtion.
properties:
  File:
    type: file
    description: The file received from the source system
    fileTypes: application/x-zip-compressed
    
  EDIMsgHeader: !include datatype-edi-msg-header.raml
