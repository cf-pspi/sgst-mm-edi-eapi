    </flow>
    <flow name="post:\purchase-results:multipart\form-data:sgst-mm-edi-eapi-config">
        <flow-ref doc:name="sgst-pr-edi-eapi-impl-flow" doc:id="dab6d87c-76f3-4951-9430-3a08fa31e50c" name="sgst-pr-edi-eapi-impl-flow" />
    </flow>
    <flow name="post:\material-master:multipart\form-data:sgst-mm-edi-eapi-config">
