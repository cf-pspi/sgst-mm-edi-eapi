        <error-handler ref="common-exception-handler" />
    </flow>
    <flow name="post:\vendor-master:multipart\form-data:sgst-mm-edi-eapi-config">
        <flow-ref doc:name="sgst-mm-edi-eapi-impl-flow" doc:id="dcc782e9-5b4d-4fca-8087-28df09254ea1" name="sgst-mm-edi-eapi-impl-flow" />
        <error-handler ref="common-exception-handler" />
    </flow>
    <flow name="post:\control-indicator:multipart\form-data:sgst-mm-edi-eapi-config">
        <flow-ref doc:name="sgst-mm-edi-eapi-impl-flow" doc:id="600bea09-2625-4d5f-b7f9-83c6a113b0f9" name="sgst-mm-edi-eapi-impl-flow" />
        <error-handler ref="common-exception-handler" />
    </flow>
    <flow name="post:\purchase-orders:multipart\form-data:sgst-mm-edi-eapi-config">
